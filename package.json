{
  "name": "element-react",
  "version": "1.0.0",
  "description": "Element UI for React",
  "main": "dist/npm/index.js",
  "scripts": {
    "start": "cd site && node run.js",
    "test": "jest --config jest.json",
    "build": "rm -fr dist && cd build/site && webpack --progress",
    "lint": "eslint -c .eslintrc.json src",
    "lint:src:q": "eslint --quiet -c .eslintrc.json src",
    "install:githooks": "node ./build/githooks/install-git-hooks.js",
    "beautify:js": "js-beautify -r src/tree/**/*.js"
  },
  "files": [
    "dist/npm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eleme/element-react.git"
  },
  "keywords": [
    "Element",
    "UI",
    "React"
  ],
  "author": "ELEME Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eleme/element-react/issues"
  },
  "homepage": "https://github.com/eleme/element-react#readme",
  "devDependencies": {
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.3.13",
    "babel-standalone": "^6.18.1",
    "css-loader": "^0.25.0",
    "element-ui": "^1.0.0-rc.8",
    "eslint": "^3.9.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "highlight.js": "^9.7.0",
    "jest": "^16.0.2",
    "js-beautify": "^1.6.4",
    "marked": "^0.3.6",
    "node-sass": "^3.10.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.0",
    "react-test-renderer": "^15.3.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "async-validator": "^1.6.6",
    "classnames": "^2.2.5",
    "is-equal": "^1.5.3",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-addons-transition-group": "^15.3.2",
    "react-dom": "^15.3.2"
  }
}
