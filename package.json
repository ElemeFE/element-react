  "scripts": {
    "start": "cd site && webpack serve",
    "typescript-test": "tsc --lib es2015,dom --moduleResolution node --project typings/typing-tests",
    "test": "flow check && BABEL_ENV=development jest",
    "jest": "jest",
    "lint": "eslint src libs site --quiet",
    "build": "cd build/site && webpack --progress",
    "prepublÂ¡sh": "./build/scripts/prepare.sh",
    "postpublish": "./build/scripts/release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ElemeFE/element-react.git"
  },
  "keywords": [
    "Element",
    "UI",
    "React"
  ],
