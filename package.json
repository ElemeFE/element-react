    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^6.2.0",
    "flow-bin": "^0.92.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.3.1",
    "marked": "^0.6.0",
