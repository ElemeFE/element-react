{
  "name": "element-react",
  "version": "1.0.0-alpha.3",
  "description": "Element UI for React",
  "private": true,
  "main": "dist/npm/index.js",
  "jsnext:main": "next.js",
  "scripts": {
    "start": "cd site && node run.js",
    "test": "jest",
    "lint": "eslint src libs site --quiet",
    "build": "cd build/site && webpack --progress",
    "prepare": "./build/scripts/prepare.sh",
    "postpublish": "./build/scripts/release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eleme/element-react.git"
  },
  "keywords": [
    "Element",
    "UI",
    "React"
  ],
  "author": "ELEME Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eleme/element-react/issues"
  },
  "homepage": "https://github.com/eleme/element-react#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.3.13",
    "babel-standalone": "^6.19.0",
    "css-loader": "^0.26.0",
    "element-theme-default": "^1.0.3",
    "eslint": "^3.9.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^17.0.3",
    "js-beautify": "^1.6.4",
    "marked": "^0.3.6",
    "node-sass": "^3.13.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.4.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "async-validator": "^1.6.6",
    "classnames": "^2.2.5",
    "is-equal": "^1.5.3",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-addons-transition-group": "^15.4.1",
    "react-click-outside": "^2.2.0",
    "react-dom": "^15.4.1"
  },
  "jest": {
    "testRegex": "/jest/.+\\.(js|jsx)$",
    "testPathIgnorePatterns": [
      "<rootDir>/tests/jest/mock.js"
    ],
    "moduleNameMapper": {
      "\\.(png|eot|svg|ttf|woff|woff2)(\\?.+)?$": "<rootDir>/tests/jest/mock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
