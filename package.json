{
  "name": "element-react",
  "version": "1.0.2",
  "description": "Element UI for React",
  "private": false,
  "main": "dist/npm/es5/index.js",
  "jsnext:main": "next.js",
  "typings": "typings/index.d.ts",
  "scripts": {
    "start": "cd site && node run.js",
    "typescript-test": "tsc --project typings/typing-tests",
    "test": "flow check && BABEL_ENV=development jest",
    "lint": "eslint src libs site --quiet",
    "build": "cd build/site && webpack --progress",
    "prepublishOnly": "./build/scripts/prepare.sh",
    "postpublish": "./build/scripts/release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eleme/element-react.git"
  },
  "keywords": [
    "Element",
    "UI",
    "React"
  ],
  "author": "ELEME Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eleme/element-react/issues"
  },
  "homepage": "https://github.com/eleme/element-react",
  "devDependencies": {
    "@types/react": "^15.0.25",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-standalone": "^6.24.2",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.4",
    "element-theme-default": "^1.3.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.47.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.4",
    "marked": "^0.3.6",
    "node-sass": "^4.5.3",
    "prismjs": "^1.6.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-scroll-up": "^1.3.0",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "typescript": "^2.3.4",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "async-validator": "^1.6.9",
    "classnames": "^2.2.5",
    "codemirror": "^5.26.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-click-outside": "^2.3.1",
    "react-dom": "^15.5.4",
    "react-transition-group": "^1.1.3",
    "throttle-debounce": "^1.0.1"
  },
  "jest": {
    "testRegex": "/jest/.+\\.(js|jsx)$",
    "testPathIgnorePatterns": [
      "<rootDir>/tests/jest/mock.js"
    ],
    "moduleNameMapper": {
      "\\.(png|eot|svg|ttf|woff|woff2)(\\?.+)?$": "<rootDir>/tests/jest/mock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
