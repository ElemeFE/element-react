{
  "env": {
    "development": {
      "presets" : ["@babel/preset-env", "@babel/preset-react", "@babel/preset-flow"],
      "plugins" : ["@babel/plugin-transform-runtime", "@babel/plugin-transform-react-jsx"]
    },
    "test": {
      "presets" : ["@babel/preset-env", "@babel/preset-react", "@babel/preset-flow"]
    },
    "production": {
      "presets" : ["@babel/preset-react", "@babel/preset-flow"],
      "plugins" : ["@babel/plugin-transform-runtime", "@babel/plugin-transform-react-jsx"]
    }
  }
}
